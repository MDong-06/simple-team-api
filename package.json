{
  "name": "simple-team-api",
  "version": "1.2.0",
  "description": "A Facebook chat API that doesn't rely on XMPP. Will NOT be deprecated after April 30th 2015.",
  "scripts": {
    "test": "mocha",
    "lint": "eslint **.js",
    "prettier": "prettier utils.js src/* --write"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/....git"
  },
  "keywords": [
    "facebook",
    "chat",
    "api",
    "fca"
  ],
  "bugs": {
    "url": "https://github.com/manhkhac/lawerpr0ject-api/issues"
  },
  "author": "Avery, David, Maude, Benjamin, UIRI, MiraiTeam, LawerTeam",
  "license": "MIT",
  "dependencies": {
    "bluebird": "latest",
    "cheerio": "latest",
    "https-proxy-agent": "latest",
    "mqtt": "^4.3.7",
    "npmlog": "^1.2.0",
    "request": "latest",
    "websocket-stream": "latest"
  },
  "engines": {
    "node": ">=10.x"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "mocha": "^7.0.1",
    "prettier": "^1.11.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "es2017": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    }
  },
  "homepage": "https://github.com/....#readme",
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
